{
  "name": "LazaroCardenas",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "now": {
    "name": "lazarocardenas",
    "alias": [
      "lazarocardenas"
    ],
    "env": {
      "MONGODB_URL": "@mongodb-url",
      "NODE_ENV": "production"
    }
  },
  "scripts": {
    "dev": "nodemon server/server.js --watch common --watch server",
    "lint": "eslint .",
    "start": "node .",
    "test": "cross-env NODE_ENV=test mocha test/**/*.test.js",
    "watch": "npm run test -- --watch",
    "precommit": "yarn test",
    "prepush": "yarn lint",
    "now:deploy": "now",
    "now:alias": "now alias",
    "now": "npm run now:deploy && npm run now:alias",
    "cov": "nyc --check-coverage --functions 80 --lines 85 yarn test"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "loopback-connector-mongodb": "^3.2.1",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "cross-env": "^5.0.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-loopback": "^8.0.0",
    "eslint-plugin-import": "2.7.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nsp": "^2.1.0",
    "nyc": "^11.0.3",
    "supertest": "^3.0.0"
  },
  "nyc": {
    "exclude": [
      "test",
      "server"
    ]
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "LazaroCardenas"
}

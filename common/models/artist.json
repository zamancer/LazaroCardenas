{
  "name": "Artist",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "email": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "default": ""
    },
    "lastName": {
      "type": "string",
      "default": ""
    },
    "photo": {
      "type": "string",
      "default": ""
    },
    "phone": {
      "type": "string",
      "default": ""
    },
    "categories": {
      "type": [
        "object"
      ],
      "default": []
    }
  },
  "validations": [],
  "relations": {
    "artPieces": {
      "type": "hasMany",
      "model": "ArtPiece",
      "foreignKey": "artistId"
    }
  },
  "acls": [],
  "methods": {
    "eliminate": {
      "accepts": [
        {
          "arg": "ids",
          "type": "object",
          "required": true,
          "description": "The array of Artists ids to eliminate"
        }
      ],
      "returns": [],
      "description": "Eliminates a set of Artists",
      "http": [
        {
          "path": "/eliminate",
          "verb": "delete"
        }
      ]
    },
    "prototype.getArtistDetail": {
      "accepts": [],
      "returns": [
        {
          "arg": "details",
          "type": "object",
          "root": true,
          "description": "The artist detail with filters"
        }
      ],
      "description": "Retrieves the artist info with filters",
      "http": [
        {
          "path": "/getArtistDetail",
          "verb": "get"
        }
      ]
    }
  }
}
